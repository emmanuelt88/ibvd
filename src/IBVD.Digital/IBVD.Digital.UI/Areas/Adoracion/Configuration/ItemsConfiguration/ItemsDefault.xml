      <ItemConfiguration Name="EstilosConfiguration">
        <Items>
          <KeyValueItem Key="{Formulario_legend}"  Value="text-align: left;font-size: 11px; font-weight:bold; font-family: Arial,sans-serif;padding:5px;"></KeyValueItem>  
          <KeyValueItem Key="{Formulario_fieldset}"  Value="text-align: left;margin: auto;font-size: 11px;border: 1px solid #eeeeee;background: #ffffff none 50% 50% repeat-x;color: #333333;font-family: Arial,sans-serif;"></KeyValueItem>
          <KeyValueItem Key="{Formulario_p}"    Value="text-align: left;font-size: 11px;font-family: Arial,sans-serif;padding:5px;"></KeyValueItem>
          <KeyValueItem Key="{Formulario_label}"    Value="margin: auto;font-size: 11px;font-family: Arial,sans-serif;width:130px;font-weight:bold;margin-left:5px;margin-right:5px;text-align:left;float:left;"></KeyValueItem>
          <KeyValueItem Key="{Formulario_span}" Value="text-align: center;margin: auto;font-size: 11px;color: #333333;font-family: Arial,sans-serif;"></KeyValueItem>
          <KeyValueItem Key="{DivHeader}" Value="border: 1px solid #ccc;background-color: #222;color: #ffffff;font-weight: bold;"></KeyValueItem>
          <KeyValueItem Key="{DivContent}" Value="border: 1px solid #eeeeee;background: #ffffff none 50% 50% repeat-x;color: #333333;font-family: Arial,sans-serif;border: 1px solid #cdd5da;background-color: #f6f6f6;color: #111111;outline: none;border-left-width:0px;border-right-width:0px; padding:10px;"></KeyValueItem>
