          <KeyValueItem Key="{MainContent}" Value="border: 1px solid #eeeeee;background: #ffffff none 50% 50% repeat-x;color: #333333;font-family: Arial,sans-serif;font-size: 1.1em;border: 1px solid #cdd5da;background-color: #f6f6f6;color: #111111;outline: none;border-top-width:0px;border-bottom-width:0px;"></KeyValueItem>
          <KeyValueItem Key="{html_p}" Value="font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:5px;"></KeyValueItem>
          <KeyValueItem Key="{html_a}" Value="font-family: Arial,sans-serif; font-size: 1.1em;"></KeyValueItem>
          <KeyValueItem Key="{TABLE}" Value="border: 1px solid; #ccc;font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:5px;"></KeyValueItem>
          <KeyValueItem Key="{TABLE_FORM}" Value="font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:0px;"></KeyValueItem>
          <KeyValueItem Key="{TH}" Value="font-size: 11px;background-color: #222;color: #ffffff;font-family: Arial,sans-serif;padding:5px;text-align:center;"></KeyValueItem>
          <KeyValueItem Key="{TD}" Value="vertical-align:top;border: 1px solid #ccc;font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:5px; text-align:center;"></KeyValueItem>
          <KeyValueItem Key="{TD_LABEL}" Value="vertical-align:top;font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:5px; text-align:left; width:140px;"></KeyValueItem>
          <KeyValueItem Key="{TD_VALUE}" Value="vertical-align:top;font-size: 11px;color: #333333;font-family: Arial,sans-serif;padding:5px; text-align:left;"></KeyValueItem>
          <KeyValueItem Key="{passwordStyle}" Value="font-family: Arial,sans-serif;border: 1px solid #ccc; background-color:#eee;padding:3px;"></KeyValueItem>
          
